{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "diary",
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "instructor": {
        "type": "string"
      },      
      "startDate": {
        "type": "string"
      },
      "endDate": {
        "type": "string"
      },
      "dueDate": {
        "type": "string"
      },
      "done": {
        "type": "boolean"
      },
      "attachmentUrl": {
        "type": "string"
      }
    },
    "required": ["name", "dueDate", "done"],
    "additionalProperties": false
  }